{
    "compilerOptions": {
        "target": "esnext",
        "moduleResolution": "Bundler",
        "noEmitOnError": true,
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "isolatedModules": true,
        "strict": true,
        "skipLibCheck": true,
    }
}