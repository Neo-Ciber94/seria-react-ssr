{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "target": "esnext",
    "moduleResolution": "Bundler",
    "noEmitOnError": false,
    "jsx": "react",
    "module": "esnext",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "strict": true,
    "skipLibCheck": true,
    "incremental": true
  },
  "include": [
    "src"
  ]
}